{
	"name": "www",
	"version": "0.0.1",
	"private": false,
	"type": "module",
	"scripts": {
		"build": "pnpm run wasm && vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "pnpm run wasm && vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"ncu-update": "pnpx npm-check-updates",
		"ncu-upgrade": "pnpx npm-check-updates -u",
		"preview": "vite preview",
		"sort-package": "pnpx sort-package-json",
		"test": "pnpx newman run ./tests/Open_RMM.postman_collection.json",
		"wasm": "wasm-pack build ../db --target nodejs"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.92",
		"@prisma/client": "^5.2.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@trpc/client": "^10.34.0",
		"@trpc/server": "^10.34.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"dayjs": "^1.11.9",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"esm-env": "^1.0.0",
		"nanoid": "^4.0.2",
		"pino": "^8.14.1",
		"pino-pretty": "^10.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.2.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-copy": "^1.4.1",
		"tailwindcss": "^3.3.3",
		"trpc-sveltekit": "^3.5.17",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unplugin-icons": "^0.16.5",
		"vite": "^4.4.2",
		"vite-plugin-wasm-pack": "^0.1.12",
		"zod": "^3.21.4"
	}
}
