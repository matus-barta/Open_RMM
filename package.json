{
  "name": "open-rmm",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm",
    "server:dev": "turbo run dev --filter=!www --filter=!agent",
    "www:dev": "turbo run dev --filter=!agent",
    "agent:dev": "turbo run dev --filter=!www"
  },
  "devDependencies": {
    "prettier": "^2.8.6",
    "turbo": "^1.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.15.0"
}
